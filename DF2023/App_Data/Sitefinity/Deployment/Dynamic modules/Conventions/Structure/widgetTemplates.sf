    "Data": "@model Telerik.Sitefinity.Frontend.Mvc.Models.ContentDetailsViewModel\r\n\r\n@using System.Collections;\r\n@using System.Linq;\r\n@using Telerik.Sitefinity.Frontend.DynamicContent.WidgetTemplates.Fields.Helpers;\r\n@using Telerik.Sitefinity;\r\n@using Telerik.Sitefinity.Data.ContentLinks;\r\n@using Telerik.Sitefinity.Frontend.Mvc.Helpers;\r\n@using Telerik.Sitefinity.Libraries.Model;\r\n@using Telerik.Sitefinity.Localization;\r\n@using Telerik.Sitefinity.Model;\r\n@using Telerik.Sitefinity.Model.ContentLinks;\r\n@using Telerik.Sitefinity.Modules.Pages;\r\n@using Telerik.Sitefinity.Pages.Model;\r\n\r\n<div class=\"@Model.CssClass\" @Html.InlineEditingAttributes(Model.ProviderName, Model.ContentType.FullName, (Guid)Model.Item.Fields.Id)>\r\n    <h3 @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\")>\r\n        @Model.Item.Fields.Title\r\n    </h3>\r\n    <div>\r\n        @Model.Item.GetDateTime(\"PublicationDate\", \"MMM d, yyyy, HH:mm tt\")\r\n        @Html.CommentsCount(\"\", @Model.Item.DataItem)\r\n    </div>\r\n    \r\n    \r\n@*Start Notes field*@\r\n<div @Html.InlineEditingFieldAttributes(\"Notes\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.Notes)</div>\r\n@*End Notes field*@\r\n\r\n@*Start JSONFormSettings field*@\r\n<div @Html.InlineEditingFieldAttributes(\"JSONFormSettings\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.JSONFormSettings)</div>\r\n@*End JSONFormSettings field*@\r\n\r\n@*Start Title field*@\r\n<div>\r\n    <strong> Title :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\")>@Model.Item.Fields.Title</span>    \r\n</div>\r\n@*End Title field*@\r\n\r\n@*Start TitleAr field*@\r\n<div>\r\n    <strong> Title ar :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"TitleAr\", \"ShortText\")>@Model.Item.Fields.TitleAr</span>    \r\n</div>\r\n@*End TitleAr field*@\r\n\r\n@*Start ShortUniqueName field*@\r\n<div>\r\n    <strong> Short unique name :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"ShortUniqueName\", \"ShortText\")>@Model.Item.Fields.ShortUniqueName</span>    \r\n</div>\r\n@*End ShortUniqueName field*@\r\n\r\n@*Start Venue field*@\r\n<div>\r\n    <strong> Venue :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"Venue\", \"ShortText\")>@Model.Item.Fields.Venue</span>    \r\n</div>\r\n@*End Venue field*@\r\n\r\n@*Start InvitationEmailSubject field*@\r\n<div>\r\n    <strong> Invitation email subject :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"InvitationEmailSubject\", \"ShortText\")>@Model.Item.Fields.InvitationEmailSubject</span>    \r\n</div>\r\n@*End InvitationEmailSubject field*@\r\n\r\n@*Start UseCustomInvitation field*@\r\n<div>\r\n    <strong> Use custom invitation/landing/conformation text :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"UseCustomInvitation\", \"YesNo\")>\r\n        @Model.Item.GetBool(\"UseCustomInvitation\")\r\n    </span> \r\n</div>\r\n@*End UseCustomInvitation field*@\r\n\r\n@*Start DisableRegistration field*@\r\n<div>\r\n    <strong> Disable registration :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"DisableRegistration\", \"YesNo\")>\r\n        @Model.Item.GetBool(\"DisableRegistration\")\r\n    </span> \r\n</div>\r\n@*End DisableRegistration field*@\r\n\r\n@*Start StartDate field*@\r\n<div>\r\n    <strong> Start date :</strong>\r\n\r\n    @if(Model.Item.Fields.StartDate!=null)\r\n    {\r\n    <span @Html.InlineEditingFieldAttributes(\"StartDate\", \"DateTime\")>\r\n        @Model.Item.GetDateTime(\"StartDate\", \"MMM d, yyyy, HH:mm tt\")\r\n    </span>\r\n    } \r\n</div>\r\n@*End StartDate field*@\r\n\r\n@*Start EndDate field*@\r\n<div>\r\n    <strong> End date :</strong>\r\n\r\n    @if(Model.Item.Fields.EndDate!=null)\r\n    {\r\n    <span @Html.InlineEditingFieldAttributes(\"EndDate\", \"DateTime\")>\r\n        @Model.Item.GetDateTime(\"EndDate\", \"MMM d, yyyy, HH:mm tt\")\r\n    </span>\r\n    } \r\n</div>\r\n@*End EndDate field*@\r\n\r\n@*Start InvitationEmail field*@\r\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"InvitationEmail\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.InvitationEmail)</div>\r\n@*End InvitationEmail field*@\r\n\r\n@*Start RegistrationClosedMessage field*@\r\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"RegistrationClosedMessage\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.RegistrationClosedMessage)</div>\r\n@*End RegistrationClosedMessage field*@\r\n\r\n@*Start Logo field*@\r\n<ul>\r\n@foreach (var relatedItem in Model.Item.Fields.Logo)\r\n{\r\n    <li class=\"media\">\r\n        <a class=\"media-body\" href='@relatedItem.Fields.MediaUrl'>\r\n            <img src='@relatedItem.Fields.ThumbnailUrl' alt='@relatedItem.Fields.AlternativeText' title='@relatedItem.Fields.Title' />\r\n        </a>\r\n    </li>   \r\n}\r\n</ul>\r\n@*End Logo field*@\r\n    \r\n\r\n    @Html.CommentsList(@Model.Item.DataItem)\r\n</div>",
