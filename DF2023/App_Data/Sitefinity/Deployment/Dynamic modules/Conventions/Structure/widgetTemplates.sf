    "Data": "@model Telerik.Sitefinity.Frontend.Mvc.Models.ContentDetailsViewModel\r\n\r\n@using System.Collections;\r\n@using System.Linq;\r\n@using Telerik.Sitefinity.Frontend.DynamicContent.WidgetTemplates.Fields.Helpers;\r\n@using Telerik.Sitefinity;\r\n@using Telerik.Sitefinity.Data.ContentLinks;\r\n@using Telerik.Sitefinity.Frontend.Mvc.Helpers;\r\n@using Telerik.Sitefinity.Libraries.Model;\r\n@using Telerik.Sitefinity.Localization;\r\n@using Telerik.Sitefinity.Model;\r\n@using Telerik.Sitefinity.Model.ContentLinks;\r\n@using Telerik.Sitefinity.Modules.Pages;\r\n@using Telerik.Sitefinity.Pages.Model;\r\n\r\n<div class=\"@Model.CssClass\" @Html.InlineEditingAttributes(Model.ProviderName, Model.ContentType.FullName, (Guid)Model.Item.Fields.Id)>\r\n    <h3 @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\")>\r\n        @Model.Item.Fields.Title\r\n    </h3>\r\n    <div>\r\n        @Model.Item.GetDateTime(\"PublicationDate\", \"MMM d, yyyy, HH:mm tt\")\r\n        @Html.CommentsCount(\"\", @Model.Item.DataItem)\r\n    </div>\r\n    \r\n    \r\n@*Start DelegationJSON field*@\r\n<div @Html.InlineEditingFieldAttributes(\"DelegationJSON\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.DelegationJSON)</div>\r\n@*End DelegationJSON field*@\r\n\r\n@*Start Title field*@\r\n<div>\r\n    <strong> Title :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\")>@Model.Item.Fields.Title</span>    \r\n</div>\r\n@*End Title field*@\r\n\r\n@*Start ContactName field*@\r\n<div>\r\n    <strong> Contact name :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"ContactName\", \"ShortText\")>@Model.Item.Fields.ContactName</span>    \r\n</div>\r\n@*End ContactName field*@\r\n\r\n@*Start ContactEmail field*@\r\n<div>\r\n    <strong> Contact email :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"ContactEmail\", \"ShortText\")>@Model.Item.Fields.ContactEmail</span>    \r\n</div>\r\n@*End ContactEmail field*@\r\n\r\n@*Start SecondaryEmail field*@\r\n<div>\r\n    <strong> Secondary email :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"SecondaryEmail\", \"ShortText\")>@Model.Item.Fields.SecondaryEmail</span>    \r\n</div>\r\n@*End SecondaryEmail field*@\r\n\r\n@*Start ContactPhoneNumber field*@\r\n<div>\r\n    <strong> Contact phone number :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"ContactPhoneNumber\", \"ShortText\")>@Model.Item.Fields.ContactPhoneNumber</span>    \r\n</div>\r\n@*End ContactPhoneNumber field*@\r\n\r\n@*Start IsSingle field*@\r\n<div>\r\n    <strong> Is single :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"IsSingle\", \"YesNo\")>\r\n        @Model.Item.GetBool(\"IsSingle\")\r\n    </span> \r\n</div>\r\n@*End IsSingle field*@\r\n\r\n@*Start InvitationSent field*@\r\n<div>\r\n    <strong> Invitation sent :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"InvitationSent\", \"YesNo\")>\r\n        @Model.Item.GetBool(\"InvitationSent\")\r\n    </span> \r\n</div>\r\n@*End InvitationSent field*@\r\n\r\n@*Start InvitationDate field*@\r\n<div>\r\n    <strong> Invitation date :</strong>\r\n\r\n    @if(Model.Item.Fields.InvitationDate!=null)\r\n    {\r\n    <span @Html.InlineEditingFieldAttributes(\"InvitationDate\", \"DateTime\")>\r\n        @Model.Item.GetDateTime(\"InvitationDate\", \"MMM d, yyyy, HH:mm tt\")\r\n    </span>\r\n    } \r\n</div>\r\n@*End InvitationDate field*@\r\n\r\n@*Start NumberOfOfficialDelegates field*@\r\n<div>\r\n    <strong> Number of official delegates :</strong>\r\n\r\n    @if(Model.Item.Fields.NumberOfOfficialDelegates!=null)\r\n    {\r\n    <span>@Model.Item.Fields.NumberOfOfficialDelegates</span>   \r\n    <span></span>\r\n    }\r\n</div>\r\n@*End NumberOfOfficialDelegates field*@\r\n\r\n@*Start RemainingNumberOfOfficialDelegates field*@\r\n<div>\r\n    <strong> Remaining number of official delegates :</strong>\r\n\r\n    @if(Model.Item.Fields.RemainingNumberOfOfficialDelegates!=null)\r\n    {\r\n    <span>@Model.Item.Fields.RemainingNumberOfOfficialDelegates</span>   \r\n    <span></span>\r\n    }\r\n</div>\r\n@*End RemainingNumberOfOfficialDelegates field*@\r\n\r\n@*Start Entity field*@\r\n<div>\r\n    <strong> Related entity :</strong>\r\n    \r\n    @if(Model.Item.Fields.Entity != null)\r\n    {\r\n        <a href='@Model.Item.Fields.Entity.DefaultUrl'>\r\n            @Model.Item.Fields.Entity.Identifier\r\n        </a>\r\n    }\r\n</div>\r\n@*End Entity field*@\r\n\r\n@*Start ServicesLevel field*@\r\n<div>\r\n    <strong> Related service level :</strong>\r\n    \r\n    @if(Model.Item.Fields.ServicesLevel != null)\r\n    {\r\n        <a href='@Model.Item.Fields.ServicesLevel.DefaultUrl'>\r\n            @Model.Item.Fields.ServicesLevel.Identifier\r\n        </a>\r\n    }\r\n</div>\r\n@*End ServicesLevel field*@\r\n\r\n@*Start Guests field*@\r\n<div>\r\n    <strong> Related guests</strong>\r\n\r\n    <ul>\r\n    @foreach (var relatedItem in Model.Item.Fields.Guests)\r\n    {\r\n        <li>\r\n            <a href='@relatedItem.DefaultUrl'>\r\n                @relatedItem.Identifier\r\n            </a>\r\n        </li>   \r\n    }\r\n    </ul>\r\n</div>\r\n@*End Guests field*@\r\n\r\n@*Start Country field*@\r\n<div>\r\n    <strong> Related country :</strong>\r\n    \r\n    @if(Model.Item.Fields.Country != null)\r\n    {\r\n        <a href='@Model.Item.Fields.Country.DefaultUrl'>\r\n            @Model.Item.Fields.Country.Identifier\r\n        </a>\r\n    }\r\n</div>\r\n@*End Country field*@\r\n    \r\n\r\n    @Html.CommentsList(@Model.Item.DataItem)\r\n</div>",
