    "Data": "@model Telerik.Sitefinity.Frontend.Mvc.Models.ContentDetailsViewModel\r\n\r\n@using System.Collections;\r\n@using System.Linq;\r\n@using Telerik.Sitefinity.Frontend.DynamicContent.WidgetTemplates.Fields.Helpers;\r\n@using Telerik.Sitefinity;\r\n@using Telerik.Sitefinity.Data.ContentLinks;\r\n@using Telerik.Sitefinity.Frontend.Mvc.Helpers;\r\n@using Telerik.Sitefinity.Libraries.Model;\r\n@using Telerik.Sitefinity.Localization;\r\n@using Telerik.Sitefinity.Model;\r\n@using Telerik.Sitefinity.Model.ContentLinks;\r\n@using Telerik.Sitefinity.Modules.Pages;\r\n@using Telerik.Sitefinity.Pages.Model;\r\n\r\n<div class=\"@Model.CssClass\" @Html.InlineEditingAttributes(Model.ProviderName, Model.ContentType.FullName, (Guid)Model.Item.Fields.Id)>\r\n    <h3 @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\")>\r\n        @Model.Item.Fields.Title\r\n    </h3>\r\n    <div>\r\n        @Model.Item.GetDateTime(\"PublicationDate\", \"MMM d, yyyy, HH:mm tt\")\r\n        @Html.CommentsCount(\"\", @Model.Item.DataItem)\r\n    </div>\r\n    \r\n    \r\n@*Start Title field*@\r\n<div>\r\n    <strong> Title :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\")>@Model.Item.Fields.Title</span>    \r\n</div>\r\n@*End Title field*@\r\n\r\n@*Start InitiatorID field*@\r\n<div>\r\n    <strong> InitiatorID :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"InitiatorID\", \"ShortText\")>@Model.Item.Fields.InitiatorID</span>    \r\n</div>\r\n@*End InitiatorID field*@\r\n\r\n@*Start TargetID field*@\r\n<div>\r\n    <strong> TargetID :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"TargetID\", \"ShortText\")>@Model.Item.Fields.TargetID</span>    \r\n</div>\r\n@*End TargetID field*@\r\n\r\n@*Start ConnectionStatus field*@\r\n<div>\r\n    <strong> Connection status :</strong>\r\n\r\n    @if(Model.Item.Fields.ConnectionStatus != null)\r\n    {\r\n        <span @Html.InlineEditingFieldAttributes(\"ConnectionStatus\", \"ChoicesRadioButtons\")>\r\n            @Model.Item.GetChoiceLabel(\"ConnectionStatus\", @Model.ContentType)\r\n        </span>\r\n    }\r\n</div>\r\n@*End ConnectionStatus field*@\r\n\r\n@*Start TargetSpeaker field*@\r\n<div>\r\n    <strong> TargetSpeaker :</strong>\r\n    \r\n    @if(Model.Item.Fields.TargetSpeaker != null)\r\n    {\r\n        <a href='@Model.Item.Fields.TargetSpeaker.DefaultUrl'>\r\n            @Model.Item.Fields.TargetSpeaker.Identifier\r\n        </a>\r\n    }\r\n</div>\r\n@*End TargetSpeaker field*@\r\n\r\n@*Start InitiatorSpeaker field*@\r\n<div>\r\n    <strong> InitiatorSpeaker :</strong>\r\n    \r\n    @if(Model.Item.Fields.InitiatorSpeaker != null)\r\n    {\r\n        <a href='@Model.Item.Fields.InitiatorSpeaker.DefaultUrl'>\r\n            @Model.Item.Fields.InitiatorSpeaker.Identifier\r\n        </a>\r\n    }\r\n</div>\r\n@*End InitiatorSpeaker field*@\r\n    \r\n\r\n    @Html.CommentsList(@Model.Item.DataItem)\r\n</div>",
